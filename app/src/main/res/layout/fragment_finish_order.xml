                android:layout_marginStart="16dp"
                android:text="TextView"
                app:layout_constraintBottom_toBottomOf="@+id/textViewMainDishCost"
                app:layout_constraintStart_toEndOf="@+id/textViewMainDishCost"
                app:layout_constraintTop_toTopOf="@+id/textViewMainDishCost"
                app:layout_constraintVertical_bias="1.0" />

            <TextView
                android:id="@+id/textViewSaladTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
