                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:text="TextView"
                app:layout_constraintBottom_toBottomOf="@+id/textViewWarmUpCost"
                app:layout_constraintStart_toEndOf="@+id/textViewWarmUpCost"
                app:layout_constraintTop_toTopOf="@+id/textViewWarmUpCost" />
