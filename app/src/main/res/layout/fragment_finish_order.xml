            <TextView
                android:id="@+id/textViewBeverageCost"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:text="TextView"
                app:layout_constraintBottom_toTopOf="@+id/textViewBeverage"
                app:layout_constraintStart_toEndOf="@+id/textView19"
                app:layout_constraintTop_toTopOf="@+id/textView19" />

            <TextView
                android:id="@+id/textViewDessertTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:text="TextView"
                app:layout_constraintBottom_toBottomOf="@+id/textViewDessertCost"
                app:layout_constraintStart_toEndOf="@+id/textViewDessertCost"
                app:layout_constraintTop_toTopOf="@+id/textViewDessertCost"
                app:layout_constraintVertical_bias="0.19" />

            <TextView
                android:id="@+id/textViewBeverageTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
